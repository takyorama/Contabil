/***********  Controle de eventos  **********/
#include "Servico.h"

class ServicosDAO{
  public:
    ServicosDAO();
    ~ServicosDAO();
    /**********  EVENTOS  **********/
    Servico Find(std::string);
    int Insert(Servico &);
    int Change(Servico &);
    int Erase(Servico &);
    std::string Error();
  private:
    std::string erro;
    char mensagem[255];
};
