/*****  Controle gestor dos dados  *****/
#include "Controller.h"

class Servico : public Controller{
  public:
    Servico();
    Servico(int, descript, price);
    
    /**********  GETTERS & SETTERS  **********/
    void setDescript(std::string);
    void setPrice(float);
    
    std::string getDescript();
    float getPrice();
  private:
    std::string descricao;
    float preco;
};
